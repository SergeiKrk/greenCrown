---
// Components
// - Layout
import Layout from '../layouts/Layout.astro'
// - UI
import Hero from '../components/blocks/hero/PageHero.astro'
import SidebarContent from '../components/blocks/basic/StickySidebar.astro'
import FAQ from '../components/blocks/FAQ/FaqSticky.astro'
import TextImage from '../components/blocks/basic/TextImage.astro'
import CTA from '../components/blocks/CTA/BasicDark.astro'
import FormOption from '../components/ui/forms/form-option/FormOption.jsx'
import Feature from '../components/ui/Feature.astro'

import heroImg from '../assets/cards/lechenie-derevev.webp'
import Grut from '../assets/Grut.webp'
// Data
import faqData from '../data/json-files/faqData.json'

import features from '../data/json-files/featuresData.json'
// - FAQ Categories
const faqIntegrations = faqData.filter((faq) => faq.category === 'obsledovanie-derevev')

// Content
import obsledImg from '../assets/обследование-деревьев.jpg'
import { Image } from 'astro:assets'
// - SEO
const SEO = {
	title:
		'Лесопатологическое обследование деревьев, диагностика и экспертиза болезней ваших растений',
	description:
		'Профессиональная экспертиза и обследование деревьев. Выявление заболеваний, анализ состояния, рекомендации по уходу. Сохраните здоровье и красоту ваших зелёных насаждений.'
}
// - Page Header
const header = {
	title: 'Лесопатологическое <strong>обследование деревьев</strong>',
	text: 'Профессиональная диагностика для выявления болезней, вредителей и повреждений.'
}
---

<Layout title={SEO.title} description={SEO.description}>
	<Hero title={header.title} text={header.text} heroImg={heroImg} />
	<section
		class="mt-42 mx-auto mb-6 max-w-[1200px] bg-neutral-100/80 p-6 2xl:my-24 2xl:bg-white"
		aria-multiselectable="false"
	>
		<div class="md:flex md:flex-row">
			<div class="text-slate-500">
				<div class="relative p-6 pb-0 pr-0">
					<figure class="relative z-10">
						<blockquote class="p-6 leading-relaxed">
							<p class="mb-2 text-sm font-light">
								<i>
									Лесопатологическое обследование и экспертиза дерева. Современные методы. Гарантия
									безопасности участка.
								</i>
							</p>
							<p class="mt-1">
								<span class="font-bold">Закажите обследование сегодня</span> и получите подробный план
								ухода за вашими деревьями!
